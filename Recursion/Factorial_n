import java.util.Scanner;
class Recursion{
    static int  Fact(int n) {
        if(n==0|| n==1 )
         return 1;
        
        return Fact(n-1)*n;
      
        
    }






    public static void main(String [] args){
         Scanner s = new Scanner(System.in);
         int n = s.nextInt();
        System.out.print(Fact(n));

    }


}
